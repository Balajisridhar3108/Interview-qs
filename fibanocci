//fibanocci
Public class fibanocci {
  public static void main(String[] args) {
  int firstNum = 0;
  int secNum = 1;
  int sum = 0;
  for(int i = 1;i<=8;i++){
      sum = firstNum + secNum;
      syso(sum);
      firstNum = secNum;
      secNum = sum;
      
     }
     
   }
   
 }
      
      
      
      
      
