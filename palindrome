public class Palindrome{
public static void main(String[] args) {
      String s = "madam";
      String empty = " ";
      for(int i =s.length-1; i>=0; i--) {
              empty = empty + s.charAt(i);
        }
           syso(empty);
        if(s.equals(empty)) {
              syso("it is palindrome");
        }
        else{
                syso("it is not a palindrome);
                
         }
       }
     
  }
