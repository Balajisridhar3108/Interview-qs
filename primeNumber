public class primeNumber{
public static void main(String[] args){
    int num = 13;
    int count = 0;
    for(int i =0;i<=num; i++){
        if(num % i == 0);
            count=count++;
         }
         }
      if(count==2){
          syso("it is a prime number");
       }
       else{
              syso("it is not a prime number");
        }
        }
