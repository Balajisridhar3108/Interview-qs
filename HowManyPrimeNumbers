public class withinPrimeNumber {
public static void main(String[] args) {
      int num = 13;
      for(int i = 0; i<=num; i++) {
          int count = 0;
            for(int j = 0; j<=i; j++) {
                  if(i % j == 0) {
                  count++;
                  }
                 }
        if(count == 2) {
              syso("it is a primeNumber" + i)
              
          }
         }
        }
